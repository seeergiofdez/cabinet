<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
    author="Sergio Fernández"
    version="1.1A"
    shortdescription="Mobiliario Urbano"
    description="Algunos presets útiles para añadir por zonas residenciales"
    icon="https://cabinet.bbjprojek.org/favicon.svg">

    <group name="Mobiliario Urbano">

      <item name="Amazon Hub Lockers" icon="presets/service/parcel_locker.svg" type="node,closedway" preset_name_label="true">
        <link wiki="Tag:transformer" />
        <space />
        <key key="amenity" value="parcel_locker" />
        <key key="parcel_pickup" value="yes" />
        <key key="brand:wikidata" value="Q16974764" />
        <key key="brand" value="Amazon" />
        <text key="name" text="Nombre" />
        <text key="ref" text="No. de Referencia" />
        <combo key="location" text="Localizacion" values="indoor,outdoor" />
        <space />
      </item>

      <item name="Contenedores de Basura" icon="presets/service/recycling/recycling_container.svg" type="node,closedway">
        <label text="Contenedores de Basura" />
        <space />
        <key key="amenity" value="recycling" />
        <key key="recycling_type" value="container" />
        <check key="recycling:glass" text="Cristal" default="yes" delete_if_empty="true" />
        <check key="recycling:paper" text="Papel y cartón" default="yes" delete_if_empty="true" />
        <check key="recycling:plastic" text="Plástico" default="yes" delete_if_empty="true" />
        <check key="recycling:waste" text="Orgánico" default="yes" delete_if_empty="true" />
        <space />
      </item>

      <item name="Transformadores de Distribución" icon="presets/power/transformer.svg" type="node,closedway" preset_name_label="true">
        <link wiki="Tag:transformer" />
        <space />
        <key key="power" value="transformer" />
        <key key="transformer" value="distribution" />
        <key key="frequency" value="50" />
        <text key="name" text="Nombre" />
        <text key="ref" text="No. de Referencia" />
        <combo key="location" text="Localización" values="indoor,outdoor,underground" />
        <space />
      </item>

      <item name="Public Toilets" type="node,closedway">
        <key key="source" value="Open Cities Dhaka Survey"/>
        <key key="amenity" value="toilets"/>
        <text key="toilets:num_chambers" text="Number of Chambers" default="" delete_if_empty="true"/>
      </item>

      <item name="Sample Building" type="node,closedway">
        <label text="Building Form" />
        <label text="---------------------------" />
        <key key="source" value="My Building Survey" />
        <key key="building" value="yes" />
        <text key="name" text="Name of Building" default="" delete_if_empty="true" />
        <combo key="building:use" text="Building Use" values="residential, commercial, industrial" display_values="Residential, Commercial, Industrial"/>
        <check key="building:vacant" text="Is the building vacant?" default="off" delete_if_empty="true" />
        <label text="---------------------------" />
      </item>
    </group>
</presets>
